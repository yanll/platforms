<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanll.business.auth.dao.MenuBeanMapper">
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
    <select id="selectMenus" resultMap="BaseResultMap">
        select * from m_menu where portal_id = #{portal_id} and enabled = 1
    </select>
    <select id="selectAllMapMenusForTree" resultType="java.util.Map">
        select * from m_menu where enabled = 1
    </select>
</mapper>